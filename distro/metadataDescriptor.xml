<?xml version="1.0"?>
<metadataDescriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.rpath.com/permanent/factorydef-1.0.xsd    factorydef-1.0.xsd">
  <metadata></metadata>
  <dataFields>
    <field>
      <name>metadata.owner</name>
      <help lang="en_US">@Help_import_image_owner@</help>
      <required>true</required>
      <readonly>true</readonly>
      <descriptions>
        <desc>Owner</desc>
      </descriptions>
      <type>str</type>
      <default/>
    </field>
    <field>
      <name>metadata.admin</name>
      <help lang="en_US">@Help_import_image_admin@</help>
      <required>true</required>
      <readonly>true</readonly>
      <descriptions>
        <desc>Admin</desc>
      </descriptions>
      <type>str</type>
      <default/>
    </field>
    <field>
      <name>metadata.location</name>
      <required>true</required>
      <descriptions>
        <desc>Location</desc>
      </descriptions>
      <help lang="en_US">@Help_import_image_location@</help>
      <enumeratedType>
        <describedValue>
          <descriptions>
            <desc>Internal</desc>
          </descriptions>
          <key>internal</key>
        </describedValue>
        <describedValue>
          <descriptions>
            <desc>DMZ</desc>
          </descriptions>
          <key>dmz</key>
        </describedValue>
        <describedValue>
          <descriptions>
            <desc>Public</desc>
          </descriptions>
          <key>public</key>
        </describedValue>
      </enumeratedType>
    </field>
    <field>
      <name>metadata.workload</name>
      <required>true</required>
      <descriptions>
        <desc>Workload</desc>
      </descriptions>
      <help lang="en_US">@Help_import_image_workload@</help>
      <enumeratedType>
        <describedValue>
          <descriptions>
            <desc>Application Server</desc>
          </descriptions>
          <key>appserver</key>
        </describedValue>
        <describedValue>
          <descriptions>
            <desc>Database Server</desc>
          </descriptions>
          <key>dbserver</key>
        </describedValue>
        <describedValue>
          <descriptions>
            <desc>File Store</desc>
          </descriptions>
          <key>filestore</key>
        </describedValue>
        <describedValue>
          <descriptions>
            <desc>Middleware/Messaging</desc>
          </descriptions>
          <key>middleware</key>
        </describedValue>
        <describedValue>
          <descriptions>
            <desc>Web Server</desc>
          </descriptions>
          <key>webserver</key>
        </describedValue>
        <describedValue>
          <descriptions>
            <desc>Other</desc>
          </descriptions>
          <key>other</key>
        </describedValue>
      </enumeratedType>
    </field>
    <field>
      <name>metadata.billing_code</name>
      <help lang="en_US">@Help_import_image_billing_code@</help>
      <descriptions>
        <desc>Billing Code</desc>
      </descriptions>
      <type>str</type>
      <default/>
    </field>
    <field>
      <name>metadata.department_code</name>
      <help lang="en_US">@Help_import_image_dept_code@</help>
      <descriptions>
        <desc>Department Code</desc>
      </descriptions>
      <type>str</type>
      <default/>
    </field>
    <field>
      <name>metadata.cost</name>
      <help lang="en_US">@Help_import_image_cost@</help>
      <descriptions>
        <desc>Cost</desc>
      </descriptions>
      <type>str</type>
      <default/>
    </field>
    <field>
      <name>metadata.credentials_username</name>
      <help lang="en_US">Administrator account name used to take control of this image at deploy time.</help>
      <descriptions>
        <desc>User Name</desc>
      </descriptions>
      <type>str</type>
    </field>
    <field>
      <name>metadata.credentials_password</name>
      <help lang="en_US">Administrator password used to take control of this image at deploy time.</help>
      <descriptions>
        <desc>Password</desc>
      </descriptions>
      <type>str</type>
    </field>
    <field>
      <name>metadata.credentials_sshkey</name>
      <help lang="en_US">Private key in PEM format used with SSH to take control of this image at deploy time.</help>
      <descriptions>
        <desc>Secure Shell (SSH) Key</desc>
      </descriptions>
      <type>str</type>
      <allowFileContent>true</allowFileContent>
      <constraints>
        <range>
          <min>100</min>
          <max>8192</max>
        </range>
      </constraints>
    </field>
  </dataFields>
</metadataDescriptor>
