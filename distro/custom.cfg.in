[global]
server.supplemental_groups = ['apache']
plugins.disable = ['/configure/ConaryConf', '/flipflop/Update', '/factoryreset/FactoryReset']
product.productName="SAS App Engine"
product.productVersion="@VERSION@"
product.companyCopyYears=""
product.companyName = "SAS Institute, Inc."
services.Services.hidden = True
apphome.visible = True
apphome.text = "SAS App Engine"

# Wizard plugins to run in this order. 
# -- Terms of service should always be first!
wizard.plugins = ["/configure/SetTimeZone", "/configure/Proxy", "/rbasetup/rBASetup"]

[/logs]
logs.files = {'Conary': '/var/log/conary', 'Inbound Mirroring': '/var/log/rbuilder/mirror-inbound.log', 'Outbound Mirroring': '/var/log/rbuilder/mirror-outbound.log', 'Jobmaster': '/var/log/rbuilder/jobmaster.log', 'MCP': '/var/log/rbuilder/mcp.log', 'Mirror Preloading': '/var/log/rbuilder/load-mirror.log', 'Package Indexer': '/var/log/rbuilder/package-index.log', 'External Package Indexer': '/var/log/rbuilder/package-index-external.log', 'Web Access': '/var/log/rbuilder/nginx_access.log', 'Application Errors': '/var/log/gunicorn/error.log', 'Conary Migration Log': '/var/log/rbuilder/conary-db-migration.log', 'rMake Server Log': '/var/log/rmake/rmake-server.log', 'rMake Node Log': '/var/log/rmake/rmake-node.log', 'Capsule Indexer': '/var/log/rbuilder/capsule-indexer.log', 'rMake3 Access Log' : '/var/log/rmake3/access.log', 'rMake3 Dispatcher Log' : '/var/log/rmake3/dispatcher.log', 'rMake3 Node Log' : '/var/log/rmake3/rmake-node.log'}

logs.num_lines = 20

[/configure/Network]
# A list of interfaces that should be excluded from display.
configure.network.ifsExclude = ["vif", "lo", "sit", "tun", "vmnet", "ird", "veth[0-9]*", "vif[0-9]*" ]

[/configure/Proxy]
configure.proxy.postscript = "/usr/share/rbuilder/scripts/apply-proxy-settings"

[/collectiontool]
collectiontool.vendorInformationPath = "/usr/share/raa/content/collectiontool.html"

[/updatetroves]
updatetroves.migrate.hide = False

[/status]
status.vendorInformationPath = "/usr/share/raa/content/status.html"
