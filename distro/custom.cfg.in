[global]
plugins.disable = ['/configure/ConaryConf', '/services/Services']
product.productName="rBuilder Appliance"
product.productVersion="@VERSION@"

postmigrate.rBuilderMigration.hidden="property"

[/logs]
logs.files = {'Appliance Logs': None, 'Conary': '/var/log/conary', 'Inbound Mirroring': '/var/log/rbuilder/mirror-inbound.log', 'Outbound Mirroring': '/var/log/rbuilder/mirror-outbound.log', 'Mirror Preloading': '/var/log/rbuilder/load-mirror.log', 'Package Indexer': '/var/log/rbuilder/package-index.log', 'External Package Indexer': '/var/log/rbuilder/package-index-external.log'}

logs.num_lines = 20

[/configure/Network]
# A list of interfaces that should be excluded from display.
configure.network.ifsExclude = ["vif", "lo", "sit", "tun", "vmnet", "ird"]
