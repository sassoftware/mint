[global]
plugins.disable = ['/configure/ConaryConf', '/services/Services', '/flipflop/Update', '/factoryreset/FactoryReset']
product.productName="rBuilder Appliance"
product.productVersion="@VERSION@"

[/logs]
logs.files = {'Appliance Agent: Web Service': '/var/log/raa/web', 'Appliance Agent: Backend Service': '/var/log/raa/raa-service.log', 'Conary': '/var/log/conary', 'Inbound Mirroring': '/var/log/rbuilder/mirror-inbound.log', 'Outbound Mirroring': '/var/log/rbuilder/mirror-outbound.log', 'Jobmaster': '/var/log/rbuilder/jobmaster.log', 'MCP': '/var/log/rbuilder/mcp.log', 'Mirror Preloading': '/var/log/rbuilder/load-mirror.log', 'Package Indexer': '/var/log/rbuilder/package-index.log', 'External Package Indexer': '/var/log/rbuilder/package-index-external.log', 'Apache Access': '/var/log/httpd/access_log', 'Apache Errors': '/var/log/httpd/error_log', 'Conary Migration Log': '/var/log/rbuilder/conary-db-migration.log'}

logs.num_lines = 20

[/configure/Network]
# A list of interfaces that should be excluded from display.
configure.network.ifsExclude = ["vif", "lo", "sit", "tun", "vmnet", "ird"]

[/collectiontool]
collectiontool.vendorInformationPath = "/usr/share/raa/content/collectiontool.html"

[/updatetroves]
updatetroves.migrate.hide = False

[/status]
status.vendorInformationPath = "/usr/share/raa/content/status.html"
