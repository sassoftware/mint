[global]
server.supplemental_groups = ['apache']
plugins.disable = ['/configure/ConaryConf', '/flipflop/Update', '/factoryreset/FactoryReset']
product.productName="rBuilder Appliance"
product.productVersion="@VERSION@"
services.Services.hidden = True

# Wizard plugins to run in this order. 
# -- Terms of service should always be first!
wizard.plugins = ["/tos/TermsOfService", "/changepassword/ChangePassword", "/configure/Notify", "/configure/Network", "/configure/Proxy", "/configure/Entitlements", "/backup/Backup", "/rbasetup/rBASetup", "/targetConfig/TargetConfig"]

[/logs]
logs.files = {'Appliance Agent: Web Service': '/var/log/raa/web', 'Appliance Agent: Backend Service': '/var/log/raa/raa-service.log', 'Conary': '/var/log/conary', 'Inbound Mirroring': '/var/log/rbuilder/mirror-inbound.log', 'Outbound Mirroring': '/var/log/rbuilder/mirror-outbound.log', 'Jobmaster': '/var/log/rbuilder/jobmaster.log', 'MCP': '/var/log/rbuilder/mcp.log', 'Mirror Preloading': '/var/log/rbuilder/load-mirror.log', 'Package Indexer': '/var/log/rbuilder/package-index.log', 'External Package Indexer': '/var/log/rbuilder/package-index-external.log', 'Apache Access': '/var/log/httpd/access_log', 'Apache Errors': '/var/log/httpd/error_log', 'Conary Migration Log': '/var/log/rbuilder/conary-db-migration.log', 'rMake Server Log': '/var/log/rmake/rmake-server.log', 'rMake Node Log': '/var/log/rmake/rmake-node.log'}

logs.num_lines = 20

[/configure/Network]
# A list of interfaces that should be excluded from display.
configure.network.ifsExclude = ["vif", "lo", "sit", "tun", "vmnet", "ird", "veth[0-9]*", "vif[0-9]*" ]

[/collectiontool]
collectiontool.vendorInformationPath = "/usr/share/raa/content/collectiontool.html"

[/updatetroves]
updatetroves.migrate.hide = False

[/status]
status.vendorInformationPath = "/usr/share/raa/content/status.html"
